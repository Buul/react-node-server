{
  "name": "website-ejuno",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "http-status": "^1.2.0",
    "jwt-simple": "^0.5.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2"
  },
  "scripts": {
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired  build",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged",
    "build-css": "node-sass-chokidar ./src/styles/scss -o ./src/styles/css --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/scss -o ./src/styles/css --output-style compressed --watch --use-polling --polling-interval 500",
    "server-dev": "set REACT_APP_DB=dev && babel-node server/index.js",
    "server-production": "set REACT_APP_DB=production && babel-node server/index.js",
    "server-test-integration": "set REACT_APP_DB=test && mocha --opts server/test/integration/mocha.opts server/test/integration/**/*.js",
    "server-test-unit": "set REACT_APP_DB=test && mocha --opts server/test/unit/mocha.opts server/test/unit/**/*.js",
    "server-test-contract": "set REACT_APP_DB=test && mocha --opts server/test/contracts/mocha.opts server/test/contracts/*.js",
    "server-lint": "eslint ./server/*.js --ignore-pattern node_modules/",
    "server-test": "npm-run-all server-lint server-test-integration server-test-unit server-test-contract"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "joi": "^13.6.0",
    "joi-assert": "^0.0.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "react-app-rewired": "^1.5.2",
    "supertest": "^3.1.0",
    "testdouble": "^3.8.1"
  },
  "url": {
    "type": "string"
  },
  "homepage": "."
}
